{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/search/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/send-email/?$",
			"dest": "_render"
		},
		{
			"src": "^/assets/?$",
			"dest": "_render"
		},
		{
			"src": "^/insights/?$",
			"dest": "_render"
		},
		{
			"src": "^/projects/?$",
			"dest": "_render"
		},
		{
			"src": "^/results/?$",
			"dest": "_render"
		},
		{
			"src": "^/sample-blog-post/?$",
			"dest": "_render"
		},
		{
			"src": "^/services/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}